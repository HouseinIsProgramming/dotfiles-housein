{
  "window.commandCenter": true,
  "workbench.preferredLightColorTheme": "ZED One Theme Light",
  "window.autoDetectColorScheme": true,
  "git.autofetch": true,
  "editor.fontSize": 17,
  "editor.fontFamily": "\"GeistMono Nerd Font Mono\"",
  "terminal.integrated.fontSize": 17,
  "editor.lineHeight": 1.3,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.fontLigatures": false,
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
//   "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
//   "vim.insertModeKeyBindings": [
//     {
//       "before": ["j", "j"],
//       "after": ["<Esc>"]
//     }
//   ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "d"],
      "after": ["\"","_", "d", "d"]
    },
    {
      "before": ["<esc>"],
      "commands": [":nohl"]
    },
  ],
  "vim.leader": "<space>",
//   "vim.handleKeys": {
//     "<C-a>": false,
//     "<C-f>": false
//   },
  // To improve performance
  "extensions.experimental.affinity": {
    "vscodevim.vim": 1
  },
  "sonarlint.focusOnNewCode": false,
  "sonarlint.connectedMode.connections.sonarcloud": [
    {
      "organizationKey": "vendure-ecommerce",
      "connectionId": "vendure-ecommerce",
      "disableNotifications": true,
      "region": "EU"
    }
  ],
  "sonarlint.output.showVerboseLogs": true,
  "workbench.colorTheme": "Nord Deep",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "catppuccin-noctis-icons.hidesExplorerArrows": false,
  "workbench.iconTheme": "catppuccin noctis icons",
  "workbench.productIconTheme": "material-product-icons",
  "cursor.composer.shouldChimeAfterChatFinishes": true,
  "cursor.composer.textSizeScale": 1.15,
  "workbench.preferredDarkColorTheme": "Cursor Dark Midnight"
}
